{
  "typography": {
    "fonts": {
      "sans": [
        "Inter",
        "-apple-system",
        "BlinkMacSystemFont",
        "Segoe UI",
        "Roboto",
        "sans-serif"
      ]
    },
    "weights": {
      "light": "300",
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700",
      "extrabold": "800"
    },
    "sizes": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem"
    },
    "lineHeights": {
      "none": "1",
      "tight": "1.25",
      "snug": "1.375",
      "normal": "1.5",
      "relaxed": "1.625",
      "loose": "2"
    },
    "tracking": {
      "tighter": "-0.05em",
      "tight": "-0.025em",
      "normal": "0em",
      "wide": "0.025em",
      "wider": "0.05em",
      "widest": "0.1em"
    },
    "variants": {
      "h1": {
        "size": "4xl",
        "weight": "extrabold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "h2": {
        "size": "3xl",
        "weight": "semibold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "h3": {
        "size": "2xl",
        "weight": "semibold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "h4": {
        "size": "xl",
        "weight": "semibold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "h5": {
        "size": "lg",
        "weight": "semibold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "h6": {
        "size": "base",
        "weight": "semibold",
        "tracking": "tight",
        "lineHeight": "tight"
      },
      "subtitle1": {
        "size": "lg",
        "weight": "medium",
        "lineHeight": "normal"
      },
      "subtitle2": {
        "size": "base",
        "weight": "medium",
        "lineHeight": "normal"
      },
      "body1": {
        "size": "base",
        "weight": "normal",
        "lineHeight": "relaxed"
      },
      "body2": {
        "size": "sm",
        "weight": "normal",
        "lineHeight": "normal"
      },
      "caption": {
        "size": "sm",
        "weight": "medium",
        "lineHeight": "none"
      },
      "overline": {
        "size": "sm",
        "weight": "medium",
        "tracking": "wider",
        "lineHeight": "none",
        "transform": "uppercase"
      }
    }
  }
}
